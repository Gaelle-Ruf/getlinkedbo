{% extends "backoffice/layout.html.twig" %}

{% block content %}

    <h2>Créer un événement</h2>
        {{ form_start(formView, {'attr': {'novalidate': 'novalidate'}}) }}
            {{ form_row(formView.name, {'attr': {'class':'geronimo'}}) }}
        {{ form_end(formView) }}
    <button class="btn btn-dark my-2 my-sm-0 col-sd-1">Valider</button>
    <p>
        <a href="{{ path('backoffice_event_index') }}" class="btn btn-dark my-2 my-sm-0 col-sd-1">Retour à la liste</a>
    </p>

{% endblock %}