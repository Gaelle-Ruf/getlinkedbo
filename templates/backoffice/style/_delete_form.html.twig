<form method="post" action="{{ path('backoffice_style_delete', {'id': style.id}) }}" class="btn btn-dark my-2 my-sm-0 col-sd-1" onsubmit="return confirm('Voulez-vous supprimer ce style ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ style.id) }}">
    <button class="btn">Supprimer</button>
</form>